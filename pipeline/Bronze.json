{
	"name": "Bronze",
	"properties": {
		"description": "Bronze - RAW Data",
		"activities": [
			{
				"name": "Raw_Copy",
				"description": "Raw_Copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"path": "$['id']"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"path": "$['name']"
								}
							},
							{
								"source": {
									"path": "$['brewery_type']"
								},
								"sink": {
									"path": "$['brewery_type']"
								}
							},
							{
								"source": {
									"path": "$['address_1']"
								},
								"sink": {
									"path": "$['address_1']"
								}
							},
							{
								"source": {
									"path": "$['address_2']"
								},
								"sink": {
									"path": "$['address_2']"
								}
							},
							{
								"source": {
									"path": "$['address_3']"
								},
								"sink": {
									"path": "$['address_3']"
								}
							},
							{
								"source": {
									"path": "$['city']"
								},
								"sink": {
									"path": "$['city']"
								}
							},
							{
								"source": {
									"path": "$['state_province']"
								},
								"sink": {
									"path": "$['state']"
								}
							},
							{
								"source": {
									"path": "$['postal_code']"
								},
								"sink": {
									"path": "$['postal_code']"
								}
							},
							{
								"source": {
									"path": "$['country']"
								},
								"sink": {
									"path": "$['country']"
								}
							},
							{
								"source": {
									"path": "$['longitude']"
								},
								"sink": {
									"path": "$['longitude']"
								}
							},
							{
								"source": {
									"path": "$['latitude']"
								},
								"sink": {
									"path": "$['latitude']"
								}
							},
							{
								"source": {
									"path": "$['phone']"
								},
								"sink": {
									"path": "$['phone']"
								}
							},
							{
								"source": {
									"path": "$['website_url']"
								},
								"sink": {
									"path": "$['website_url']"
								}
							},
							{
								"source": {
									"path": "$['state']"
								},
								"sink": {
									"path": "$['state']"
								}
							},
							{
								"source": {
									"path": "$['street']"
								},
								"sink": {
									"path": "$['street']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Rest_Raw_Copy",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "OutputRawBrew",
